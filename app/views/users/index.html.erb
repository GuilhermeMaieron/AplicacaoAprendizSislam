<%= form_for @user, url: logar_path do |l| %>

  <div class="Novo">
    <p>
      <%= @error %>
      <%= l.label :Username%><br>
      <%= l.text_field :username %>
    </p>

    <p>
      <%= l.label :Senha%><br>
      <%= l.password_field :senha %>
    </p>
    <p>

    <%= l.submit %>
    </p>
  </div>
<%end%>
